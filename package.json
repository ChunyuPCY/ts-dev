{
  "name": "ts-dev",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commitlint": "commitlint --edit",
    "prepare": "simple-git-hooks"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20.0.0",
