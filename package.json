  },
  "devDependencies": {
    "vite": "catalog:cli",
    "lint-staged": "catalog:cli",
    "@types/node": "catalog:types",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "eslint": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "@commitlint/cli": "catalog:cli",
    "@commitlint/config-conventional": "catalog:cli",
    "@commitlint/types": "catalog:types"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
